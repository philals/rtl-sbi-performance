{
  "name": "rtl-sbi-performance",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest",
    "preview": "vite preview",
    "storybook": "npm run generate && start-storybook -p 6006",
    "generate": "npm run generate-storybook-stories && npm run generate-rtl-testcases",
    "generate-storybook-stories": "mustache ./src/TaskList.json ./src/TaskList.stories.tsx.mustache > ./src/TaskList.stories.tsx",
    "generate-rtl-testcases": "mustache ./src/TaskList.json ./src/TaskList.test.tsx.mustache > ./src/TaskList.test.tsx",
    "build-storybook": "export NODE_OPTIONS=--openssl-legacy-provider && build-storybook",
    "test-storybook": "test-storybook --url http://127.0.0.1:6006"
  },
  "dependencies": {
    "@storybook/jest": "^0.0.10",
    "@storybook/test-runner": "^0.9.2",
    "babel-preset-react-app": "10.0.1",
    "identity-obj-proxy": "3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.41.5",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-interactions": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/react": "6.5.15",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-jest": "29.3.1",
    "babel-loader": "8.3.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "mustache": "^4.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
